<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
                                                   android:orientation="vertical"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent" android:id="@+id/linearLayout4">
    <View android:id="@+id/rectangle_391"
          android:background="@drawable/rectangle_391_shape"
          android:layout_width="0dp"
          android:layout_height="101dp" app:layout_constraintEnd_toEndOf="parent"
          android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
          android:layout_marginStart="8dp"
          app:layout_constraintHorizontal_bias="1.0" android:layout_marginTop="16dp"
          app:layout_constraintTop_toTopOf="parent"/>

    <TextView android:id="@+id/pharma2"
              android:fontFamily="@font/gothamrounded_book"
              android:textSize="12sp"
              android:textColor="@android:color/holo_blue_dark"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text="Voir l'ordonnance"
              app:layout_constraintTop_toBottomOf="@+id/pharma" android:layout_marginEnd="8dp"
              app:layout_constraintEnd_toEndOf="@+id/rectangle_391"
              android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintHorizontal_bias="0.275" android:textStyle="italic" android:layout_marginBottom="8dp"
              app:layout_constraintBottom_toBottomOf="@+id/rectangle_391" app:layout_constraintVertical_bias="0.0"/>

    <TextView android:id="@+id/textView7"
              android:fontFamily="@font/gothamrounded_book"
              android:textSize="20sp"
              android:textColor="#19769f"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text="Commande 01"
              android:layout_marginEnd="152dp"
              app:layout_constraintEnd_toEndOf="@+id/rectangle_391" android:layout_marginRight="152dp"
              android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/rectangle_391"
              app:layout_constraintStart_toEndOf="@+id/images" android:layout_marginStart="8dp"
              android:layout_marginLeft="8dp"/>

    <ImageView android:id="@+id/images"
               android:src="@drawable/images1"
               android:contentDescription="images"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               app:layout_constraintTop_toTopOf="@+id/rectangle_391"
               android:layout_marginTop="8dp"
               android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="@+id/rectangle_391"
               android:layout_marginLeft="8dp"/>

    <TextView android:id="@+id/pharma"
              android:fontFamily="@font/gothamrounded_book"
              android:textSize="12sp"
              android:textColor="#9d9d9d"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text="Pharmacie El Bahdja"
              android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toBottomOf="@+id/divider2" android:layout_marginTop="4dp"
              android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="@+id/rectangle_391"
              app:layout_constraintHorizontal_bias="0.292"/>

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:fontFamily="@font/gothamrounded_book"
            android:layout_height="wrap_content"
            android:id="@+id/textView24"
            app:layout_constraintEnd_toEndOf="@+id/rectangle_391"
            android:layout_marginEnd="16dp" android:textSize="10sp" app:layout_constraintTop_toBottomOf="@+id/divider2"
            android:layout_marginTop="20dp"/>
    <com.google.android.material.chip.Chip
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:id="@+id/chip2" android:layout_marginTop="4dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/textView7"
            android:layout_marginStart="136dp" app:chipBackgroundColor="@color/annul"
            app:layout_constraintEnd_toEndOf="@+id/rectangle_391" android:layout_marginEnd="8dp"/>
    <View
            android:id="@+id/divider2"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:background="@color/grey"
            app:layout_constraintEnd_toEndOf="@+id/rectangle_391" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toEndOf="@+id/images"
            app:layout_constraintTop_toBottomOf="@+id/textView7" android:layout_marginTop="12dp"
            app:layout_constraintHorizontal_bias="1.0"/>

</androidx.constraintlayout.widget.ConstraintLayout>