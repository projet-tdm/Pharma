<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".CommandeFragment" android:background="@drawable/rectangle_395_shape" android:id="@+id/frameLayout3">
    <ListView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:id="@+id/listcmd"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/en_cours"
            app:layout_constraintVertical_bias="1.0"/>
    <androidx.appcompat.widget.Toolbar
            android:layout_width="0dp"
            android:layout_height="68dp"
            android:background="@drawable/toolbar"
            android:theme="?attr/actionBarTheme"
            android:minHeight="?attr/actionBarSize"
            android:id="@+id/toolbar3"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="1.0"/>
    <TextView
            android:text="Commandes"
            android:layout_width="wrap_content"
            android:fontFamily="@font/gothamrounded_mediumitalic"
            android:layout_height="wrap_content"
            android:id="@+id/textView" android:layout_marginTop="20dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="48dp" android:layout_marginStart="48dp"
            android:textColor="@android:color/background_light" android:textSize="24sp"/>

    <ImageView android:id="@+id/ligne_1"
               android:src="@drawable/ligne_1"
               android:contentDescription="ligne_1"
               android:layout_width="222dp"
               android:layout_height="1dp"
               app:layout_constraintTop_toBottomOf="@+id/toolbar3" android:layout_marginTop="24dp"
               android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
               android:layout_marginRight="8dp" android:layout_marginStart="8dp"
               app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
               app:layout_constraintHorizontal_bias="0.387"/>

    <View android:id="@+id/ellipse_77"
          android:background="@drawable/ellipse_77_shape"
          android:layout_width="0dp"
          android:layout_height="25dp"
          android:visibility="visible" android:layout_marginTop="8dp"
          app:layout_constraintTop_toBottomOf="@+id/toolbar3" android:layout_marginEnd="100dp"
          app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="100dp"
          app:layout_constraintStart_toEndOf="@+id/ellipse_76" android:layout_marginStart="90dp"
          android:layout_marginLeft="90dp" app:layout_constraintHorizontal_bias="0.911"/>

    <View android:id="@+id/ellipse_78"
          android:background="@drawable/ellipse_78_shape"
          android:layout_width="27dp"
          android:layout_height="25dp" android:layout_marginTop="8dp"
          app:layout_constraintTop_toBottomOf="@+id/toolbar3" app:layout_constraintStart_toStartOf="parent"
          android:layout_marginLeft="60dp" android:layout_marginStart="60dp"/>

    <View android:id="@+id/ellipse_76"
          android:background="@drawable/ellipse_76_shape"
          android:layout_width="29dp"
          android:layout_height="25dp" android:layout_marginTop="8dp"
          app:layout_constraintTop_toBottomOf="@+id/toolbar3" app:layout_constraintStart_toEndOf="@+id/ellipse_78"
          android:layout_marginLeft="80dp" android:layout_marginStart="80dp"/>

    <TextView android:id="@+id/trait_e"
              android:fontFamily="@font/gothamrounded_lightitalic"
              android:textSize="12sp"
              android:textColor="#9d9d9d"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text="Traitée"
              app:layout_constraintStart_toEndOf="@+id/en_cours"
              android:layout_marginStart="8dp" android:layout_marginLeft="8dp" android:layout_marginEnd="8dp"
              app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
              app:layout_constraintHorizontal_bias="0.409" app:layout_constraintTop_toBottomOf="@+id/ellipse_77"
              android:layout_marginTop="4dp"/>

    <TextView android:id="@+id/en_cours"
              android:fontFamily="@font/gothamrounded_lightitalic"
              android:textSize="12sp"
              android:textColor="#9d9d9d"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text="En cours"
              app:layout_constraintStart_toEndOf="@+id/annul_e"
              android:layout_marginStart="60dp" android:layout_marginLeft="60dp" android:layout_marginTop="4dp"
              app:layout_constraintTop_toBottomOf="@+id/ellipse_76"/>

    <TextView android:id="@+id/annul_e"
              android:fontFamily="@font/gothamrounded_lightitalic"
              android:textSize="12sp"
              android:textColor="#9d9d9d"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:text="Annulée"
              app:layout_constraintStart_toStartOf="parent"
              android:layout_marginLeft="48dp" android:layout_marginStart="48dp" android:layout_marginTop="4dp"
              app:layout_constraintTop_toBottomOf="@+id/ellipse_78"/>
    <Button
            android:layout_width="72dp"
            android:layout_height="67dp"
            android:id="@+id/button2"
            android:background="@drawable/ic_add_circle_black_24dp"
            app:layout_constraintStart_toEndOf="@+id/textView"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="@+id/toolbar3"
            android:layout_marginEnd="8dp" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintBottom_toBottomOf="@+id/toolbar3"/>
</androidx.constraintlayout.widget.ConstraintLayout>

